{
  "name": "rivena-ai-server",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build ./agent.ts --outdir ./build --target=bun",
    "dev:agent": "NODE_ENV=development bun ./agent.ts dev",
    "start:dev": "NODE_ENV=development bun ./agent.ts dev",
    "start:prod": "NODE_ENV=production bun ./agent.ts",
    "start": "concurrently \"bun run dev\" \"bun run start:dev\"",
    "lint": "eslint -f unix \"**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@elysiajs/swagger": "^1.2.2",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@livekit/agents": "^0.7.0",
    "@livekit/agents-plugin-deepgram": "^0.5.4",
    "@livekit/agents-plugin-elevenlabs": "^0.6.1",
    "@livekit/agents-plugin-livekit": "^0.1.0",
    "@livekit/agents-plugin-openai": "^0.8.2",
    "@livekit/agents-plugin-silero": "^0.5.4",
    "@livekit/rtc-node": "^0.13.6",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "elysia": "latest",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "livekit-server-sdk": "^2.10.2",
    "loglevel": "^1.9.2",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "module": "src/index.js"
}