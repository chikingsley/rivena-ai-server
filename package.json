{
  "name": "my-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "module": "src/server.js",
  "main": "build.ts",
  "scripts": {
    "dev": "concurrently \"bunx --bun vite\" \"bun run --watch src/server.ts\"",
    "preview": "vite preview",
    "clean": "rm -rf ./build",
    "prebuild": "bun run clean",
    "build": "concurrently \"bun build ./src/agents/agent.ts --outdir ./build --target=bun\" \"tsc -b && vite build\"",
    "start:dev": "NODE_ENV=development bun src/agents/agent.ts dev",
    "start:prod": "NODE_ENV=production bun src/agents/agent.ts",
    "start": "concurrently \"bun run dev\" \"bun run start:dev\"",
    "lint": "eslint -f unix \"**/*.ts\"",
    "lint:fix": "eslint -f unix \"**/*.ts\" --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "fix": "bun run format && bun run lint:fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@elysiajs/swagger": "^1.2.2",
    "@eslint/eslintrc": "^3.3.0",
    "@livekit/agents": "^0.7.0",
    "@livekit/agents-plugin-deepgram": "^0.5.4",
    "@livekit/agents-plugin-elevenlabs": "^0.6.1",
    "@livekit/agents-plugin-livekit": "^0.1.0",
    "@livekit/agents-plugin-openai": "^0.8.2",
    "@livekit/agents-plugin-silero": "^0.5.4",
    "@livekit/rtc-node": "^0.13.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/vite": "^4.0.9",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "bun-types": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "elysia": "^1.2.24",
    "eslint-config-prettier": "^10.0.2",
    "eslint-formatter-unix": "^8.40.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "livekit-server-sdk": "^2.10.2",
    "lucide-react": "^0.477.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.9",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}